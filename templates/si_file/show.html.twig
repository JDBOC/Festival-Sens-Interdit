{% extends 'base.html.twig' %}

{% block title %}SiFile{% endblock %}

{% block body %}
    <h1>SiFile</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ si_file.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ si_file.name }}</td>
            </tr>
            <tr>
                <th>MimeType</th>
                <td>{{ si_file.mimeType }}</td>
            </tr>
            <tr>
                <th>Link</th>
                <td>{{ si_file.link }}</td>
            </tr>
            <tr>
                <th>Type</th>
                <td>{{ si_file.type }}</td>
            </tr>
            <tr>
                <th>MediaFileName</th>
                <td>{{ si_file.mediaFileName }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('si_file_index') }}">back to list</a>

    <a href="{{ path('si_file_edit', {'id': si_file.id}) }}">edit</a>

    {{ include('si_file/_delete_form.html.twig') }}
{% endblock %}
